{
    "env": {
        "es6": true,
        "jasmine": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "console": true,
        "PubSub": true,
        "createjs": true
    },
    "rules": {
        "quotes": [ 2, "single" ]
    }
}